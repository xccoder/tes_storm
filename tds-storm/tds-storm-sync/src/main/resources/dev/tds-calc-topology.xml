<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd"
	default-lazy-init="true" default-init-method="init">

	<import resource="classpath:topology-base.xml" />

	<bean id="proxyJedisPoolFactory" class="com.edcs.tds.common.redis.JedisFactory">

	</bean>

	<bean id="proxyJedisPool" factory-bean="proxyJedisPoolFactory" factory-method="createProxyJedisPool"/>


</beans>